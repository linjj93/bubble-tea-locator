{
  "name": "bubble-tea-locator",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "react-scripts": "5.0.1",
    "reactstrap": "^8.0.1"
  },
  "scripts": {
    "test": "react-scripts test",
    "test:watch": "react-scripts test --watch",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "coverage": "CI=true react-scripts test --env=jsdom --coverage --watchAll=false",
    "local:coverage": "react-scripts test --coverage --watchAll=false"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^8.0.1",
    "history": "^4.9.0",
    "jest-dom": "^3.5.0",
    "jest-mock-axios": "^3.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/components/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
